{
  "name": "@renovatebot/ruby-semver",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Ruby SemVer in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renovatebot/ruby-semver"
  },
  "scripts": {
    "build": "tsc",
    "eslint": "eslint .",
    "eslint-fix": "eslint .",
    "jest": "jest --coverage",
    "jest-debug": "node --expose-gc --inspect-brk node_modules/jest/bin/jest --runInBand",
    "lint": "run-s eslint prettier",
    "lint-fix": "run-s eslint-fix prettier-fix",
    "prettier": "prettier --check --ignore-unknown \"**/*.*\"",
    "prettier-fix": "prettier --write --ignore-unknown \"**/*.*\"",
    "test": "run-s lint type-check jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "tslib": "2.4.1"
  },
  "devDependencies": {
    "@renovate/eslint-plugin": "https://github.com/renovatebot/eslint-plugin#v0.0.5",
    "@types/jest": "27.5.2",
    "@typescript-eslint/eslint-plugin": "5.48.1",
    "@typescript-eslint/parser": "5.48.1",
    "babel-jest": "27.5.1",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "conventional-changelog-conventionalcommits": "5.0.0",
    "eslint": "8.31.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-formatter-gha": "1.4.1",
    "eslint-plugin-import": "2.27.4",
    "eslint-plugin-jest": "27.2.1",
    "eslint-plugin-promise": "6.1.1",
    "jest": "27.5.1",
    "jest-junit": "15.0.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.8.2",
    "semantic-release": "19.0.5",
    "ts-jest": "27.1.5",
    "typescript": "4.9.4"
  }
}
