{
  "name": "@renovatebot/ruby-semver",
  "version": "1.0.0",
  "license": "MIT",
  "description": "Ruby SemVer in TypeScript.",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/renovatebot/ruby-semver"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json",
    "eslint": "eslint --ext .js,.ts lib/",
    "eslint-fix": "eslint --ext .js,.ts --fix lib/",
    "jest": "jest --coverage",
    "jest-debug": "node --expose-gc --inspect-brk node_modules/jest/bin/jest --runInBand",
    "lint": "run-s eslint prettier",
    "lint-fix": "run-s eslint-fix prettier-fix",
    "prettier": "prettier --list-different \"**/*.{ts,js,json,md}\"",
    "prettier-fix": "prettier --write \"**/*.{ts,js,json,md}\"",
    "test": "run-s lint type-check jest",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "tslib": "2.3.1"
  },
  "devDependencies": {
    "@renovate/eslint-plugin": "https://github.com/renovatebot/eslint-plugin#v0.0.4",
    "@types/jest": "27.4.0",
    "@typescript-eslint/eslint-plugin": "5.9.1",
    "@typescript-eslint/parser": "5.9.1",
    "babel-jest": "27.4.6",
    "babel-plugin-dynamic-import-node": "2.3.3",
    "codecov": "3.8.3",
    "eslint": "8.6.0",
    "eslint-config-airbnb-typescript": "16.1.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-only-warn": "1.0.3",
    "jest": "27.4.7",
    "jest-junit": "13.0.0",
    "jest-silent-reporter": "0.5.0",
    "npm-run-all": "4.1.5",
    "prettier": "2.5.1",
    "ts-jest": "27.1.2",
    "ts-node": "10.4.0",
    "typescript": "4.5.4"
  }
}
